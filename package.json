{
  "name": "namey-numbers",
  "version": "0.0.0",
  "description": "a web page that teaches you how to convert from a number as words to a number as numerals",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "pretest": "standard --fix",
    "test": "jest",
    "build": "webpack",
    "start": "webpack-dev-server --open",
    "analyse": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pauldambra/namey-numbers.git"
  },
  "keywords": [],
  "author": "",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/pauldambra/namey-numbers/issues"
  },
  "homepage": "https://github.com/pauldambra/namey-numbers#readme",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@jest/globals": "^26.1.0",
    "@testing-library/dom": "^7.20.1",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "prerender-loader": "^1.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "standard": "^14.3.4",
    "standard-loader": "^7.0.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "standard": {
    "ignore": [
      "docs/",
      "node_modules/"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
